{
	"arrowParens": "always",
	"bracketSpacing": false,
	"embeddedLanguageFormatting": "auto",
	"htmlWhitespaceSensitivity": "css",
	"insertPragma": false,
	"jsxBracketSameLine": true,
	"jsxSingleQuote": true,
	"proseWrap": "always",
	"quoteProps": "consistent",
	"requirePragma": false,
	"semi": false,
	"singleQuote": true,
	"trailingComma": "all",
